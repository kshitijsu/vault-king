{
    "manifest_version": 3,
    "name": "Vault King",
    "version": "0.1.0",
    "action": {
        "default_popup": "popup/index.html"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls"],
            "js": ["content.js"]
        }
    ]
}
